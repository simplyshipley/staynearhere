{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0" : {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "harvesthq-chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.3",
        "drupal/admin_toolbar": "^1.23",
        "drupal/block_class": "^1.0@alpha",
        "drupal/block_visibility_groups": "^1.2",
        "drupal/bootstrap": "^3.5",
        "drupal/config_split": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.5.3",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/field_group": "^3.0",
        "drupal/field_validation": "^1.0-alpha6",
        "drupal/google_analytics": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.1",
        "drupal/taxonomy_machine_name": "^1.0@alpha",
        "drupal/twig_tweak": "2.0.0-rc3",
        "drupal/views_extras": "^1.1",
        "drupal/views_slideshow": "^4.6",
        "drupal/views_taxonomy_term_name_depth": "^3.3",
        "drupal/chosen": "^2.5",
        "harvesthq/chosen": "^1.8"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3",
        "drupal/devel": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
